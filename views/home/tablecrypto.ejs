<table class="pure-table">
    <thead>
        <th>Name</th>
        <th>Symbol</th>
        <th>Price in USD</th>
        <th>Price in EURO</th>
        <th>Supply</th>
        <th>Change</th>
    </thead>
    <tbody>
        <%if(locals.coindata){coindata.data.forEach(coin => {%>
            
            <tr id="<%=coin.name%>">
                <td><%=coin.id%></td>
                <td><%=coin.symbol%></td>
                <td>$<%=coin.priceUsd%></td>
                <td>â‚¬<%=coin.priceEuro%></td>
                <td><%=coin.supply%></td>
                <td><%=coin.changePercent24Hr%></td>
                <td><button class="button" onclick="togglemodal('bitcoin-modal')">Details</button></td>
            </tr>
        <%})}%>
    </tbody>
</table>